{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "iam:ListAttachedRolePolicies",
                "iam:ListAttachedUserPolicies",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:776347453069:log-group:*:*:*",
                "arn:aws:iam::*:user/*",
                "arn:aws:iam::*:role/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "config:ListDiscoveredResources",
                "config:PutEvaluations",
                "logs:CreateLogGroup"
            ],
            "Resource": "*"
        }
    ]
}